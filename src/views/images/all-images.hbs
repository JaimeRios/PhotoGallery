<div class="row">
    {{#each images}}
     <div class="col-md-3">
         <div class="card">
             <div class="card-body">
                 <h4 class="card-title">
                     {{title}}
                 </h4>
                 <img src="{{imageUrl}}" alt="Girl in a jacket" width="50" height="50">
                 <p>{{description}}</p>
             </div>
         </div>
     </div>
    {{/each}}
</div>